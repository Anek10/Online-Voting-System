<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Vlogin</servlet-name>
        <servlet-class>Vlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Result</servlet-name>
        <servlet-class>Result</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegVoters</servlet-name>
        <servlet-class>RegVoters</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminCheck</servlet-name>
        <servlet-class>AdminCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Vlogin</servlet-name>
        <url-pattern>/Vlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Result</servlet-name>
        <url-pattern>/Result</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegVoters</servlet-name>
        <url-pattern>/RegVoters</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminCheck</servlet-name>
        <url-pattern>/AdminCheck</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
